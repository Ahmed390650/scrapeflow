generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_DIRECT")
  directUrl = env("DATABASE_URL_DIRECT")
}
model Workflow{
  id     String @id @default(cuid())
  userId String 
  name   String
  description String?
  definition String
  status String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@unique([userId, name])
}